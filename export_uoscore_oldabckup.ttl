@prefix dc:    <http://purl.org/dc/elements/1.1/> .
@prefix statistics: <http://purl.org/net/OCRe/statistics.owl#> .

uoscore:publishInfoReverse
        a                        owl:ObjectProperty ;
        rdfs:domain              bibo:Collection ;
        rdfs:label               "Erscheinungsort für"@en-US ;
        rdfs:range               kdsf:Publikation ;
        vitro:displayLimitAnnot  "-1"^^xsd:int ;
        owl:inverseOf            uoscore:publishInfo .

uoscore:  a                        owl:Ontology ;
        rdfs:label                 "UOS Core"@de-DE ;
        vitro:ontologyPrefixAnnot  "uoscore" .

uoscore:hatDenomination
        a                              owl:ObjectProperty ;
        rdfs:domain                    kdsf:Person ;
        rdfs:label                     "hat Denomination"^^rdf:langString ;
        rdfs:range                     uoscore:Denomination ;
        vitro:hiddenFromDisplayBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:hiddenFromPublishBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:prohibitedFromUpdateBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:selectFromExistingAnnot  true .

uoscore:Denomination  a          owl:Class ;
        rdfs:label               "Denomination"^^rdf:langString ;
        vitro:displayLimitAnnot  "-1"^^xsd:int ;
        vitro:displayRankAnnot   "-1"^^xsd:int ;
        vitro:hiddenFromDisplayBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:hiddenFromPublishBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:prohibitedFromUpdateBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> .

uoscore:link  a                       owl:FunctionalProperty , owl:DatatypeProperty ;
        rdfs:domain                   kdsf:Publikation ;
        rdfs:label                    "Link"@en-US ;
        rdfs:range                    xsd:anyURI ;
        obo:IAO_0000112               "URL der \"Quelle\" einer Publikation z.B. Scopus link." ;
        vitro:hiddenFromDisplayBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:hiddenFromPublishBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:inPropertyGroupAnnot    <http://vivoweb.org/ontology#vitroPropertyGrouplinks> ;
        vitro:prohibitedFromUpdateBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#curator> ;
        vitro:publicDescriptionAnnot  "Dieser Eintrag ist für einen Link, unter dem die Publikation abgerufen / gefunden werden kann." .

uoscore:publicationlanguage
        a                              owl:ObjectProperty ;
        rdfs:domain                    kdsf:Publikation ;
        rdfs:label                     "Language"@en-US ;
        rdfs:range                     vcard:Language ;
        vitro:hiddenFromDisplayBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:hiddenFromPublishBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:inPropertyGroupAnnot     <http://vivoweb.org/ontology#vitroPropertyGroupbibobscure> ;
        vitro:prohibitedFromUpdateBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:selectFromExistingAnnot  true .

uoscore:publishInfo  a                 owl:ObjectProperty ;
        rdfs:domain                    kdsf:Publikation ;
        rdfs:label                     "veröffentlicht in"@en-US ;
        rdfs:range                     bibo:Collection ;
        rdfs:subPropertyOf             owl:topObjectProperty ;
        vitro:hiddenFromDisplayBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
        vitro:hiddenFromPublishBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#curator> ;
        vitro:inPropertyGroupAnnot     <http://vivoweb.org/ontology#vitroPropertyGroupoverview> ;
        vitro:prohibitedFromUpdateBelowRoleLevelAnnot
                <http://vitro.mannlib.cornell.edu/ns/vitro/role#curator> ;
        vitro:publicDescriptionAnnot   "Abbildung der Beziehung zwischen einer Publikation und dessen Erscheinungsort bzw. Quelle. " ;
        vitro:selectFromExistingAnnot  true ;
        owl:inverseOf                  uoscore:publishInfoReverse .
